{
  "id": "redesignrating.json",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "Redesign rating",
  "description": "",
  "type": "object",
  "properties": {
    "layoutRating": {
      "type": "integer",
      "description": "Layout rating",
      "minimum": 1,
      "maximum": 5
    },
    "typographyRating": {
      "type": "integer",
      "description": "Typography rating",
      "minimum": 1,
      "maximum": 5
    },
    "colorRating": {
      "type": "integer",
      "description": "Color rating",
      "minimum": 1,
      "maximum": 5
    },
    "brandColors": {
      "type": "array",
      "description": "New user-selected brand colors for the site",
      "items": {
        "description": "#RGB color",
        "format": "rgbhexcolor",
        "pattern": "^#([a-fA-F0-9]{6}|[a-fA-F0-9]{3})$",
        "example": "#aa33cc",
        "type": "string"
      },
      "minItems": 1,
      "maxItems": 5
    },
    "filterRating": {
      "type": "integer",
      "description": "Image filters rating",
      "minimum": 1,
      "maximum": 5
    }
  },
  "additionalProperties": false
}